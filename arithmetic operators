#include <stdio.h>
int main() 
{
  int number1,number2;
  printf("Enter value for number1: ");
  scanf("%d",&number1);
  printf("Enter value for number2: ");
  scanf("%d",&number2 );
  int sum,product,difference,quotient;
  sum=number1+ number2;
  difference=number1- number2;
  product=number1* number2;
  quotient=number1/ number2;
  printf("%d", sum );
  printf("%d", difference );
  printf("%d", product );
  printf("%d", quotient );

return 0;
}
