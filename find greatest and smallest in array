#include <stdio.h>
main() 
{
  int count, greatest, smallest, a[5];
  printf("Enter the array elements: ");
  for(count = 0; count < 5; count++)
  {
    scanf("%d", &a[i]);
  }
  greatest = a[0];
  smallest = a[0];
  for(count = 0; count < 5; count++)
  {
    if(a[i] > greatest) 
    { 
      greatest = a[i];
    }
    if(a[i] < smallest) 
    {
      smallest = a[i];
    }
  }
  printf("\nGreatest Element = %d", greatest);
  printf("\nSmallest Element = %d", smallest);}
